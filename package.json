{
  "name": "dinopianito_next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.13.2",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.4.0",
    "@types/react": "^17.0.14",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint": "7.31.0",
    "eslint-config-next": "11.0.1",
    "fs": "*",
    "mercadopago": "^1.5.8",
    "net": "^1.0.2",
    "tls": "^0.0.1",
    "typescript": "^4.3.5"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
